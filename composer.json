{
    "name": "boolfalse/laravel-shopping-cart",
    "description": "Laravel Shopping Cart",
    "keywords": ["laravel", "shopping"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "boolfalse",
            "email": "boolfalse@mail.ru"
        }
    ],
    "require": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Boolfalse\\LaravelShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boolfalse\\Tests\\LaravelShoppingCart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Boolfalse\\LaravelShoppingCart\\LSCServiceProvider"
            ],
            "aliases": {
                "Cart": "Boolfalse\\LaravelShoppingCart\\Facades\\Cart"
            }
        }
    }
}
